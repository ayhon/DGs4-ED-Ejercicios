bool resuelveCaso() {
	int n; cin >> n;
	if(!cin) return false;

	vector<int> alturas(n); // Hasta 1e9, cabe en entero
	for(auto & e : alturas) cin >> e;

	bool sol = resolver(alturas);

	cout << (sol? "SIEMPRE PREMIO" : "ELEGIR OTRA") << '\n';

	return true;
}

int main() {
	while(resuelveCaso());
	return 0;
}

